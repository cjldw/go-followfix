<seelog type="asynctimer" asyncinterval="5000000" minlevel="debug" maxlevel="error">
    <outputs formatid="main">
        <console/>
        <splitter formatid="format1">
            <file path="../log/test.log"/>
        </splitter>
    </outputs>
    <formats>
        <format id="main" format="%Level] %Msg%n"/>
        <format id="format1" format="[%LEV] %Msg%n"/>
    </formats>
</seelog>